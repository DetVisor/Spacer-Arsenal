  <ThingDef ParentName="BaseMakeableGrenade">
    <defName>DV_Weapon_GrenadeThump</defName>
    <label>thump grenades</label>
    <description>Breaching explosive grenades. Powerful concussion bombs that are extremely effective against walls and structures, but do much less damage to small targets.</description>
    <tickerType>Normal</tickerType>
    <graphicData>
      <texPath>Things/Item/Equipment/WeaponRanged/ThumpGrenades</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <uiIconScale>1.3</uiIconScale>
    <soundInteract>Interact_Grenade</soundInteract>
    <techLevel>Spacer</techLevel>
    <statBases>
      <Mass>5</Mass>
      <RangedWeapon_Cooldown>2.66</RangedWeapon_Cooldown>
      <WorkToMake>15500</WorkToMake>
    </statBases>
    <generateCommonality>0.2</generateCommonality>
    <costList>
      <Plasteel>20</Plasteel>
      <Steel>20</Steel>
      <ComponentSpacer>2</ComponentSpacer>
    </costList>
    <recipeMaker>
      <researchPrerequisite>ChargedShot</researchPrerequisite>
      <skillRequirements>
        <Crafting>7</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>FabricationBench</li>
      </recipeUsers>
      <displayPriority>200</displayPriority>
    </recipeMaker>
    <weaponTags>
      <li>GrenadeDestructive</li>
      <li>EmpireGrenadeDestructive</li>
    </weaponTags>
    <thingCategories>
      <li>Grenades</li>
    </thingCategories>
    <smeltable>true</smeltable>
    <verbs>
      <li>
        <label>throw thump grenade</label>
        <verbClass>Verb_LaunchProjectile</verbClass>
        <hasStandardCommand>true</hasStandardCommand>
        <range>11.9</range>
        <forcedMissRadius>1.9</forcedMissRadius>
        <warmupTime>2.6</warmupTime>
        <noiseRadius>6</noiseRadius>
        <ai_IsBuildingDestroyer>true</ai_IsBuildingDestroyer>
        <soundCast>ThrowGrenade</soundCast>
        <targetParams>
          <canTargetLocations>true</canTargetLocations>
        </targetParams>
        <defaultProjectile>DV_Proj_GrenadeThump</defaultProjectile>
        <rangedFireRulepack>Combat_RangedFire_Thrown</rangedFireRulepack>
      </li>
    </verbs>
    <comps>
      <li Class="CompProperties_Explosive">
        <explosiveRadius>3.9</explosiveRadius>
        <explosiveDamageType>Thump</explosiveDamageType>
        <requiredDamageTypeToExplode>Flame</requiredDamageTypeToExplode>
      </li>
    </comps>
  </ThingDef>

